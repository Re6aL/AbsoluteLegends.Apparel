<ThingDef ParentName="ApparelBase">
    <defName>Apparel_BekkitSportSuit</defName>
    <description>Best reliable clothing. Universal suit for all situations.</description>
    <label>Reliable Sport Suit</label>
    <graphicData>
      <texPath>Things/Apparel/BekkShell/Serpsport</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	<techLevel>Industrial</techLevel>
	<recipeMaker>
    <researchPrerequisite>ComplexClothing</researchPrerequisite>
    <skillRequirements>
    <Crafting>6</Crafting>
      </skillRequirements>
	  <workSkill>Crafting</workSkill>
	  <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
	  <recipeUsers Inherit="False">
        <li>ElectricTailoringBench</li>
      </recipeUsers>
	 </recipeMaker>
  <tickerType>Normal</tickerType>
  <thingCategories>
      <li>Apparel</li>
  </thingCategories>
  	<costList>
      <Cloth>50</Cloth>
    </costList>
  <statBases>
    <WorkToMake>2500</WorkToMake>
	<MarketValue>200</MarketValue>
    <MaxHitPoints>250</MaxHitPoints>
	<Mass>0.20</Mass>
	<Flammability>0.5</Flammability>
    <StuffEffectMultiplierArmor>0.35</StuffEffectMultiplierArmor>
    <StuffEffectMultiplierInsulation_Cold>9</StuffEffectMultiplierInsulation_Cold>
    <StuffEffectMultiplierInsulation_Heat>9</StuffEffectMultiplierInsulation_Heat>
    <EquipDelay>1.5</EquipDelay>
  </statBases>
  <equippedStatOffsets>
	  <AimingDelayFactor>-0.05</AimingDelayFactor>
	  <SocialImpact>0.05</SocialImpact>
      <MeleeDodgeChance>0.10</MeleeDodgeChance>
	  <WorkSpeedGlobal>0.15</WorkSpeedGlobal>
  </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/BekkShell/Serpsport</wornGraphicPath>
      <layers>
        <li>Shell</li>
      </layers>
      <tags>
        <li>Industrial</li>
      </tags>
      <defaultOutfitTags>
			<li>Soldier</li>
      </defaultOutfitTags>
	  <useDeflectMetalEffect>true</useDeflectMetalEffect>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>3</weight>
          <only>(255,255,255)</only>
        </li>
      </options>
    </colorGenerator>
	<thingSetMakerTags>
      <li>RewardSpecial</li>
    </thingSetMakerTags>
  </ThingDef>
